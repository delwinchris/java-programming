import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
public class Main { 
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in); 
        int i = input.nextInt(); 
        float f = input.nextFloat(); 
        System.out.println(i); 
        System.out.printf("%.2f%n", f); 
        input.close(); 


    }
}
